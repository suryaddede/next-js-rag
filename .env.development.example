# Development Environment Configuration
# Copy this file to .env.local for development

# ChromaDB Configuration (Development)
CHROMA_URL=http://localhost:8000

# Model Provider Configuration (required for the RAG functionality)
# Get your Google AI API key from: https://aistudio.google.com/app/apikey
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_api_key_here

# Get your Voyage AI API key from: https://dash.voyageai.com/
VOYAGE_AI_API_KEY=your_voyage_ai_api_key_here

# App Configuration (Development)
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Optional: Enable debug logging in development
DEBUG=true

# Optional: Set log level for development
LOG_LEVEL=debug
